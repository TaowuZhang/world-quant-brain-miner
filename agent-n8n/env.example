# Agent Network Environment Variables

# Moonshot API Key (Required)
MOONSHOT_API_KEY=sk-your-moonshot-api-key-here

# WorldQuant Brain Credentials (Required)
# Create credentials/credential.txt with: ["username", "password"]

# Agent Hub Configuration
AGENT_HUB_URL=http://localhost:8000
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# n8n Configuration
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=admin123

# Database Configuration
POSTGRES_DB=agent_network
POSTGRES_USER=agent_user
POSTGRES_PASSWORD=agent_password

# Redis Configuration
REDIS_URL=redis://redis:6379

# Monitoring Configuration
GRAFANA_ADMIN_PASSWORD=admin123

# Optional: Custom URLs for external access
NEXT_PUBLIC_AGENT_HUB_URL=http://localhost:8000
NEXT_PUBLIC_N8N_URL=http://localhost:5678
NEXT_PUBLIC_WS_URL=ws://localhost:8000/ws

# Optional: Development settings
DEBUG=false
LOG_LEVEL=INFO 